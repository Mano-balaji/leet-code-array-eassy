class maths{
public:
    int prime(){
    int n;
    int cnt=0;
    cout<<"enter your number:";
    cin>>n;
    if(n<1){
        cout<<n<<" its not a prime number";
    }else{
        for(int i=1;i<=n;i++){
            if(n%i==0){
                cnt++;
            }
        }
        if(cnt>2){
            cout<<n<<" its not a prime number.";
        }else{
            cout<<n<<" its a prime number.";
        }
    }
    }

};
