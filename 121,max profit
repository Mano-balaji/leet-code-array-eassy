#include<iostream>
#include <algorithm>
using namespace std; 
class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int maxpro=0;
        for(int i=0;i<prices.size();i++){
            for(int j=i+1;j<prices.size();j++){
                if(prices[i]>prices[j]){
                    break;
                }else{
                    int n=prices[j]-prices[i];
                    maxpro=max(maxpro,n);
                }
            }
        }
        return maxpro;
    }
};
