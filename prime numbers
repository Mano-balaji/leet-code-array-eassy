class n_maths{
public:
    int n_prime(){
        int last_num;
        cout<<"enter the last number:";
        cin>>last_num;
        for(int i=1;i<=last_num;i++){
                int cnt=0;
            for(int j=1;j<=i;j++){
                if(i%j==0){
                    cnt++;
                }
            }
            if(cnt==2){
                cout<<i<<endl;
            }
        }

    }
};
